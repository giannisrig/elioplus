mixin techItem(name)
    div(class='w-full').flex.gap-10px.items-center
        div(class='w-[15px] h-[15px] rounded-full').bg-blue
        a( href='#' class='text-base lg:text-lg hover:text-blue').font-semibold=name

mixin pageItem(name, active)
    if active
        div( class='w-[25px] h-[25px] lg:w-[35px] lg:h-[35px] rounded-full').bg-blue.shadow-card.flex.justify-center.text-center.items-center.text-white
            span(class='text-xs lg:text-sm').font-semibold=name
    else
        a( href='#' class='w-[25px] h-[25px] lg:w-[35px] lg:h-[35px] rounded-full hover:bg-blue hover:text-white').bg-white.shadow-card.flex.justify-center.text-center.items-center
            span(class='text-xs lg:text-sm ').font-semibold=name

doctype html
html(lang="en")
