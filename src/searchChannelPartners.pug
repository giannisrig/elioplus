mixin letterItem(letter, prefix)

    if prefix === 'cat'
        -var extraClass = 'go-to'
    else
        -var extraClass = 'go-to-expand'
    a(href='#'+prefix+letter class='text-sm lg:text-base w-[35px] h-[35px] font-medium '+extraClass).font-inter.border.border-iconGray.bg-white.rounded-full.text-textGray.flex.items-center.justify-center.text-center=letter



