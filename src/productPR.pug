
mixin featureItem(name, included)
    .flex.items-center.gap-5px
        div(class='w-[20px] h-[20px]').flex.items-center.justify-center
            if included
                img(src='../assets/images/pricing/check.svg' alt='Included feature for ' + name loading='lazy')
            else
                img(src='../assets/images/pricing/close.svg' alt='Not included feature for ' + name loading='lazy')
        if included
            div(class='text-[15px]')=name
        else
            div(class='text-[15px]').text-textGray=name


